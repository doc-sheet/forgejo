<div class="ui form">
	<div class="field">
		<label>{{$.locale.Tr "org.times.start_date"}}</label>
		<div class="ui calendar">
			<input type="date" id="rangefrom" name="from" value="{{.RangeFrom}}">
		</div>
	</div>
	<div class="field">
		<label>{{$.locale.Tr "org.times.end_date"}}</label>
		<div class="ui calendar">
			<input type="date" id="rangeto" name="to" value="{{.RangeTo}}">
		</div>
	</div>
	<button class="ui primary button">{{$.locale.Tr "org.times.update"}}</button>
</div>
